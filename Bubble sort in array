 #include<iostream>
 #include<vector>
 using namespace std;
 void bubble_sort(int arr[],int lenght){
  
  for(int i=0;i<lenght-1;i++){
    for(int j=0;j<lenght-i-1;j++){
      if(arr[j]>arr[j+1]){
        swap(arr[j],arr[j+1]);
      }
    }
  }

 }

 void shorted(int arr[],int lenght){
  for(int i=0;i<=lenght;i++){
    cout<<arr[i]<<endl;
  }
 }
 int main(){
  int arr[]={1,4,6,3,2,5};
  int lenght=sizeof(arr)/sizeof(arr[0]);
   bubble_sort(arr,lenght);
   shorted(arr, lenght);

  return 0;
 }